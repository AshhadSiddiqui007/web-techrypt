{
  "name": "techrypt-full-stack",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run start:node\" \"npm run start:react\" \"npm run start:python\"",
    "start:node": "cd Techrypt_sourcecode/server && npm start",
    "start:react": "cd Techrypt_sourcecode/Techrypt && npm run dev",
    "start:python": "cmd /c \"chcp 65001 >nul && set PYTHONIOENCODING=utf-8 && py smart_llm_chatbot.py\"",
    "setup": "npm run setup:root && npm run setup:server && npm run setup:react && npm run setup:python",
    "setup:root": "npm install",
    "setup:server": "cd Techrypt_sourcecode/server && npm install",
    "setup:react": "cd Techrypt_sourcecode/Techrypt && npm install",
    "setup:python": "py -m pip install -r requirements.txt",
    "install:all": "npm run setup",
    "build": "cd Techrypt_sourcecode/Techrypt && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
